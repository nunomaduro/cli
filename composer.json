{
    "name": "web-expendables/cli",
    "description": "A CLI tool used in intern by the member of the Web Expendables group.",
    "keywords": ["framework", "laravel", "console", "zero"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/web-expendables/cli/issues",
        "source": "https://github.com/web-expendables/cli"
    },
    "authors": [
        {
            "name": "Steven Yung",
            "email": "stevenyung@icloud.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel-zero/framework": "4.0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["cli"]
}
